{
  "name": "Epic Screenshot",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "Take screenshot of a video",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "Epic Screenshot shortcuts",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [],
  "background": {
    "service_worker": "background_script.min.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.min.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}
